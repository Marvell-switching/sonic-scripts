+    def i2c_set(self, device_addr, offset, value):
+        if smbus_present == 0:
+                cmd = "i2cset -y 0 " + hex(device_addr) + " " + hex(offset) + " " + hex(value)
+                os.system(cmd)
+        else:
+                bus = smbus.SMBus(0)
+                bus.write_byte_data(device_addr, offset, value)
