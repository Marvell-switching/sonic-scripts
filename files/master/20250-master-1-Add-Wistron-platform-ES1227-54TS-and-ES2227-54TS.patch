+    {}
+};
+
+MODULE_DEVICE_TABLE(i2c, wistron_i2c_cpld_id);
+
+static struct i2c_driver wistron_i2c_cpld_driver = {
+    .class      = I2C_CLASS_HWMON,
+    .driver = {
+        .name = "wistron_cpld",
+    },
+    .probe      = wistron_i2c_cpld_probe,
+    .remove     = wistron_i2c_cpld_remove,
+    .id_table   = wistron_i2c_cpld_id,
+};
