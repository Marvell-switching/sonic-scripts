+                val=2
+            led_path="{}/fan{}_led".format(CPLD_SYSFS_DIR, fan_index+1)
+            self.__write_txt_file(led_path, str(val))
+
+        if fan_sta:
+            val = 1
+        else:
+            val = 2
+        self.__write_txt_file(fanled_path, str(val))
+
+        for index in range(0, NUM_PSU):
+            sta=self._psu_list[index].get_status()
+            psu_sta = psu_sta & sta
+        if psu_sta:
