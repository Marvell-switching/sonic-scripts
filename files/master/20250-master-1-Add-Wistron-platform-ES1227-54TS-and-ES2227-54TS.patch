+
+COFF_KA = {
+    'exhaust': {
+        0: 2.62,
+        1: 2.15,
+        2: 2.23,
+        3: 2.51,
+        4: 1.6
+    },
+    'intake': {
+        0: 2.5,
+        1: 1.8,
+        2: 1.9,
+        3: 1.5,
+        4: 1.1
+    }
+}
+
+COFF_KB = {
+    'exhaust': {
+        0: 110,
+        1: 72,
+        2: 78,
+        3: 126,
+        4: 60
+    },
+    'intake': {
+        0: 100,
+        1: 44,
+        2: 52,
+        3: 35,
+        4: 10
+    }
+}
