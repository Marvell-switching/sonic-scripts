+            if not sta:
+                self._fan_drawer_list[fan_index]._fan_list[0].try_clear_fault()
+
+        for fan_index in range(0, NUM_FANTRAY):
+            sta=self._fan_drawer_list[fan_index].get_status()
+            prs=self._fan_drawer_list[fan_index].get_presence()
+            fan_sta = fan_sta & sta
+            if sta and prs:
