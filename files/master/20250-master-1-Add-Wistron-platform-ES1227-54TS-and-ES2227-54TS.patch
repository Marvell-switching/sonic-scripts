+    if (val < 0) {
+        mutex_unlock(&list_lock);
+        return val;
+    }
+
+    mutex_unlock(&list_lock);
+
+    ver = (val & 0x4) >> 2;
+
+    return sprintf(buf, "%d\n", ver);
+}
