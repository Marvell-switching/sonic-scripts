+}
+
+update_modulelist()
+{
+    MODULE_FILE="/etc/modules-load.d/marvell.conf"
+
+    echo "# Module list to load during the boot" > $MODULE_FILE
+    echo "mvcpss" >> $MODULE_FILE
+    echo "psample" >> $MODULE_FILE
+}
+
+main()
