+        sysled_path="{}/sys_led".format(CPLD_SYSFS_DIR)
+        psuled_path="{}/psu_led".format(CPLD_SYSFS_DIR)
+        fanled_path="{}/fan_led".format(CPLD_SYSFS_DIR)
