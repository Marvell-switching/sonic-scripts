+        """
+        direction=self.FAN_DIRECTION_EXHAUST
+
+        return direction
+
+    def get_speed(self):
+        """
+        Retrieves the speed of fan as a percentage of full speed
+        Returns:
+            An integer, the percentage of full fan speed, in the range 0 (off)
+                 to 100 (full speed)
+
+        """
+        speed = 0
+        if self.is_psu_fan:
+            psu_fan_path= "{}/{}".format(self.psu_hwmon_path, 'fan1_input')
+            rpm=self.__read_txt_file(psu_fan_path)
+            if rpm is None:
+                return 0
+            speed = int(int(rpm) * 100 / PSU_FAN_MAX_RPM)
+        elif self.get_presence():
+            #path= "{}/fan{}_input".format(self.fan_hwmon_path, FAN_ROTOR_MAPPING[self.fan_tray_index + self.fan_index])
+            path= "{}/pwm1".format(self.fan_hwmon_path)
+            pwm=self.__read_txt_file(path)
+            if pwm is None:
+                return 0
+            elif int(pwm) == 77:
