+    def __search_hwmon_dir_name(self, directory):
+        try:
+            dirs = os.listdir(directory)
+            for file in dirs:
+                if file.startswith("hwmon"):
+                    return file
+        except:
+            pass
+        return ''
+
+    def __read_txt_file(self, file_path):
+        try:
+            with open(file_path, 'r') as fd:
+                data = fd.read()
+                return data.strip()
+        except IOError:
